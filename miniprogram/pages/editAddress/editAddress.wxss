/* pages/editAddress/editAddress.wxss */
.save-box {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.save-button {
  display: flex;
  justify-content: center;
  width: 80%;
  padding: 10rpx 0rpx;
  border-radius: 100rpx;
  background-color: #FF7373;
  font-size: 36rpx;
  font-weight: 600;
  color: #ffffff;
}

.address-cell {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}